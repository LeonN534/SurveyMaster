              <AuxValues>
                <AuxValue name="JavaCodeGenerator_InitCodePre" type="java.lang.String" value="jPanel3.setBorder(new javax.swing.border.LineBorder(new java.awt.Color(51, 51, 51), 2, true));&#xa;jPanel3.setLayout(null);"/>
                <AuxValue name="JavaCodeGenerator_LayoutCodePre" type="java.lang.String" value="try (Connection conexion = DriverManager.getConnection(URL, USERNAME, PASSWORD)) {&#xa;    // Consulta SQL parametrizada&#xa;    String sql = &quot;SELECT numeroPregunta, idencuesta, texto, tipo_respuesta, opciones FROM pregunta WHERE idencuesta = ?&quot;;&#xa;&#xa;    // Crear el PreparedStatement&#xa;    try (PreparedStatement statement = conexion.prepareStatement(sql)) {&#xa;        // Establecer el valor del par&#xe1;metro&#xa;        statement.setString(1, this.encuestaID);&#xa;&#xa;        // Ejecutar la consulta&#xa;        try (ResultSet resultSet = statement.executeQuery()) {&#xa;            while (resultSet.next()) {&#xa;                // Obtener los valores de cada columna&#xa;&#xa;                int numeroPregunta = resultSet.getInt(&quot;numeroPregunta&quot;);&#xa;                int idEncuesta = resultSet.getInt(&quot;idencuesta&quot;);&#xa;                String texto = resultSet.getString(&quot;texto&quot;);&#xa;                String tipoRespuesta = resultSet.getString(&quot;tipo_respuesta&quot;);&#xa;                String opciones = resultSet.getString(&quot;opciones&quot;);&#xa;&#xa;                PanelVerRespuesta pregunta = new PanelVerRespuesta(numeroPregunta, texto, tipoRespuesta, opciones, String.valueOf(idEncuesta), preguntaY, jPanel3, titulo);&#xa;&#xa;                int posYaAumentar = pregunta.getHeight() + 15;&#xa;&#xa;                pregunta.setVisible(true);&#xa;                jPanel3.add(pregunta);&#xa;                jPanel3.repaint();&#xa;                jPanel3.revalidate();&#xa;                preguntaY = preguntaY + pregunta.getHeight() + 15;&#xa;&#xa;                jPanel3.setPreferredSize(new Dimension(1055, jPanel3.getPreferredSize().height + posYaAumentar));&#xa;            }&#xa;        }&#xa;    }&#xa;&#xa;} catch (SQLException e) {&#xa;    System.err.println(&quot;Error al conectar a la base de datos: &quot; + e.getMessage());&#xa;}"/>
